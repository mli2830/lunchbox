model{

## parameter priors
repMean ~ dunif(0,1)
effprop ~ dunif(0,1)
beta ~ dunif(0,0.2)

## ~ node magic
N0 ~ dbin(effprop,N)
I[1] ~ dbin(1,i0)
S[1] <- N0 - I[1]
pSI[1] <- 1 - (1-beta)^I[1]
mtilde[1] ~ dbeta(k/(1-prob), k/prob)
obs[1] ~ dbin(mtilda[1] , I[1])

for(t in 2:numobs){
  I[t] ~ dbin(pSI[t-1],S[t-1])
  S[t] <- S[t-1] - I[t]
  pSI[t] <- 1 - (1-beta)^I[t]
  obs[t] ~ dbin(repMean,I[t])
##  obs1[t] ~ dpois(repMean*obs1[t]+1)
  }
}
