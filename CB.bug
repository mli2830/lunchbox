model{
	## Change these uniforms to betas
	repMean ~ dbeta(1,1)
	effprop ~ dbeta(1,1)
	beta ~ dunif(0,0.2)

	N0 ~ dbin(effprop,N)
	I[1] ~ dbin(0.5,i0)
	S[1] <- N0 - I[1]
	pSI[1] <- 1 - exp(I[1]*log(1-beta))
	obs[1] ~ dbin(repMean,I[1])

	for(t in 2:numobs){
		I[t] ~ dbin(pSI[t-1],S[t-1])
		S[t] <- S[t-1] - I[t]
		pSI[t] <- 1 - exp(I[t]*log(1-beta))
		obs[t] ~ dbin(repMean,I[t]) 
	}
}