model{
  ## Change these uniforms to betas
  repMean ~ dbeta(1,1)
  effprop ~ dbeta(1,1)
  
  R0 ~ dgamma(2,1)
  N0 ~ dbin(effprop,N)
  
  S[1] <- N0 - i0
  IMean[1] <- i0*R0*(S[1]/N0)
  
  for(t in 1:numobs){
    I[t] ~ dpois(IMean[t])
    obs[t] ~ dpois(I[t]*repMean)
    IMean[t+1] <- I[t]*R0*(S[t]/N0)
    S[t+1] <- S[t] - I[t]
  }
}