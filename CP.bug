model{
  ## Change these uniforms to betas
  repMean ~ dbeta(1,1)
  beta ~ dunif(0,0.2)
  
  I[1] ~ dpois(i0)
  pSI[1] <- 1 - exp(I[1]*log(1-beta))
  obs[1] ~ dpois(I[1]*repMean)
  
  for(t in 2:numobs){
    I[t] ~ dpois(I[i-1]*pSI[i-1])
    pSI[t] <- 1 - exp(I[t]*log(1-beta))
    obs[t] ~ dpois(I[t]*repMean) 
  }
}